{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cortex Debug (JLink)",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/VdcxxxService_Example",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "jlink",
            "device": "LPC4078",
            "interface": "swd",
            "svdFile": "${workspaceFolder}/../svd/LPC408x_7x.svd",
            "runToEntryPoint": "main",
            "showDevDebugOutput": "none"
        },
        {
            "name": "Cortex Debug (OpenOCD)",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/VdcxxxService_Example",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "device": "LPC4078",
            "configFiles": [
                "interface/jlink.cfg",
                "target/lpc40xx.cfg"
            ],
            "svdFile": "${workspaceFolder}/../svd/LPC408x_7x.svd",
            "runToEntryPoint": "main",
            "showDevDebugOutput": "none"
        },
        {
            "name": "GDB Multiarch Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/VdcxxxService_Example",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb-multiarch",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
